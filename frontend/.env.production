# # ===== FRONTEND/.env.production =====
# REACT_APP_API_URL=http://IP_SERVER_BPS:5000/api

# # Contoh jika IP server BPS adalah 192.168.1.100:
# # REACT_APP_API_URL=http://192.168.1.100:5000/api

# REACT_APP_API_URL=http://localhost:5000/api

REACT_APP_ENV=production

# frontend/.env.production
# üåê PRODUCTION ENVIRONMENT CONFIG - SESUAI BACKEND ENVIRONMENT VARIABLE

# üî• CRITICAL: Sesuaikan dengan BACKEND_URL di cPanel Node.js
REACT_APP_API_URL=https://siapik1810.web.bps.go.id/api

# App configuration
REACT_APP_TITLE=SiAPIK - Sistem Penilaian dan Penentuan Pegawai Terbaik
REACT_APP_COMPANY=BPS Kabupaten Pringsewu
REACT_APP_VERSION=1.0.0

# Production optimizations
GENERATE_SOURCEMAP=false
PUBLIC_URL=/

# Feature flags for production
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false
REACT_APP_ENABLE_CONSOLE_LOGS=false

# Build optimizations
INLINE_RUNTIME_CHUNK=false
IMAGE_INLINE_SIZE_LIMIT=0